{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": ["dist", "src"],
  "engines": {"node": ">=10"},
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "animejs": "^4.0.2",
    "remotion": "4.0.325",
    "@remotion/player": "^4.0.325",
    "jotai": "^2.12.5",
    "@heroicons/react": "^2.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.26.2",
    "rehype-raw": "^7.0.0",
    "tailwind-merge": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "editor",
  "author": "soasme",
  "module": "dist/editor.esm.js",
  "size-limit": [
    {
      "path": "dist/editor.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/editor.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/animejs": "^3.1.13",
    "@types/react": "^18.0.37",
    "husky": "^9.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^11.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^3.9.10"
  }
}
